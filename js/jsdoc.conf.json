{
    "opts": {
      "template": "node_modules/daybrush-jsdoc-template"
    },
    "source": {
      "include": ["./node_modules"], 
      "includePattern": ".+\\.js(doc|x)?$",
      "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [
      "plugins/markdown"
    ],
    "templates": {
      "cleverLinks": false,
      "monospaceLinks": false
    }
  }
  